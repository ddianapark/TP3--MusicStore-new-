@{
    ViewData["Title"] = "Index";
}

<div>    
    <img src="images/logo.jpg" alt="LOGO">
    <h1>MusicStore</h1>
    <p>Bienvenidos al mejor sitio de música</p>

    @foreach(Catalogo catalogo in ViewBag.Catalogo.Values)
    {
        <div>
            <h3>@catalogo.Disco.Titulo</h3>
            <img src = "@Integrantes.DatosPersonales.Foto">
            <p>Ir a los datos personales: <a href = '@Url.Action("SelectIntegrante", "Home", new {dni = Integrantes.DNI})' alt = "Ir a los datos personales" >Información Personal </a></p>
            <p>Ir a los datos familiares: <a href = '@Url.Action("MostrarDatosFamiliares", "Home", new {dni = Integrantes.DNI})' alt = "Ir a los datos familiares" >Información Familiar </a></p>
            <p>Ir a los datos intereses: <a href = '@Url.Action("MostrarDatosIntereses", "Home", new {dni = Integrantes.DNI})' alt = "Ir a los datos intereses" >Información Intereses </a></p>
        

        
        </div>
    }

</div>
